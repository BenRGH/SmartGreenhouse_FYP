                </div>
                <div id="rawDataTable" class="row">
                </div>
            </div>

            <div class="separator"></div>

        </main>

        <% include bits/foot.ejs %>
        <!--Only used on this page anyway-->
        <script type="text/javascript" src="https://unpkg.com/tabulator-tables@4.2.1/dist/js/tabulator.min.js"></script>
        <script type="text/javascript" src="https://momentjs.com/downloads/moment.js"></script>
        <script type="text/javascript" src="javascripts/moreData.js"></script>
